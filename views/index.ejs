<!DOCTYPE html>
<html ng-app="Mastering-Data-Mining-and-Analytics">
    <head>
        <title><%= title %></title>
        <meta charset="UTF-8">
        <link rel="shortcut icon" href="images/favicon.ico" type="image/x-icon">
        <link rel="icon" href="images/favicon.ico" type="image/x-icon">
        <!--Script Module-->
        <% dependances.scripts.forEach(function(script, index) { %>
        <script src="<%= script %>.js"></script>
        <% }); %>

        <!--CSS Module-->
        <% dependances.css.forEach(function(css, index) { %>
        <link rel="stylesheet" type="text/css" href="<%= css %>.css">
        <% }); %>

    </head>
<body ng-controller="AppCtrl" layout="row">
    <div class="main-view" ng-view>

    </div>
</body>
</html>
